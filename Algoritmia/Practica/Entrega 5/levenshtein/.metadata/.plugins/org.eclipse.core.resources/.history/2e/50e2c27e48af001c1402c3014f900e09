	public static void main(String[] args) {
		Levenshtein l = new Levenshtein();
		System.out.println("Distancia: " + l.distanciaLevenshtein(args[0], args[1]));
		l.print();

	}

	public String generateRandomString() {
		//Se buscan los carácteres ascii
		int a = 97;
		int z = 122;
		int longitudCadenas = r.nextInt();// n==m
		StringBuilder buffer = new StringBuilder(longitudCadenas);
		for (int i = 0; i < longitudCadenas; i++) {
			
			int randomchar = r.nextInt();
		}
	}

}
