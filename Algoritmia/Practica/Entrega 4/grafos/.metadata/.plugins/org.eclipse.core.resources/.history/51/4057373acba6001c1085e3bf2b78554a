public class ColoracionMapa {
	private HashMap<String, ArrayList<String>> paises;//num paises-> tamaño del problema
	private HashMap<String, String> solucion;
	
	public ColoracionMapa() {
		sacarColores();
		sacarPaisesYFronteras();
	}

	private void sacarColores() {
		FileUtil fileUtil = new FileUtil("/grafos/datos/colores.txt");
		fileUtil.readLines("/grafos/datos/colores.txt");
		
	}
}
